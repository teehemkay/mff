---
export interface CardProps {
  links: string[][];
}
---

<ul>
  {
    Astro.props.links.map(([label, url]) => (
      <li class="mb-4">
        <a
          href={url}
          class="text-lg"
          target={url.includes('europarl.europa.eu') ? null : '_blank'}>
          {label}
        </a>
      </li>
    ))
  }
</ul>
